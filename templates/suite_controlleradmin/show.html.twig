{% extends 'base.html.twig' %}

{% block title %}Suites{% endblock %}

{% block body %}
    <h1>Suite</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ suite.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ suite.name }}</td>
            </tr>
            <tr>
                <th>Occupied</th>
                <td>{{ suite.occupied ? 'Oui' : 'Non' }}</td>
            </tr>
            <tr>
                <th>OccupiedBy</th>
                <td>{{ suite.occupiedBy }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ suite.price }}</td>
            </tr>
            <tr>
                <th>PicturesUrl</th>
                <td>{{ suite.picturesUrl ? suite.picturesUrl|json_encode : '' }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ suite.Title }}</td>
            </tr>
            <tr>
                <th>BookingUrl</th>
                <td>{{ suite.bookingUrl }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ suite.description }}</td>
            </tr>
            <tr>
                <th>Hotel_id</th>
                <td>{{ suite.hotelId }}</td>
            </tr>
            <tr>
                <th>MainPictureUrl</th>
                <td>{{ suite.mainPictureUrl }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_suite_controlleradmin_index') }}">back to list</a>

    <a href="{{ path('app_suite_controlleradmin_edit', {'id': suite.id}) }}">edit</a>

    {{ include('suite_controlleradmin/_delete_form.html.twig') }}
{% endblock %}
